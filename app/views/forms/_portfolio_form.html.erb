<!-- this is the unfinished portfolio page -->

<% form_for @new_portfolio, :html => { :multipart => true } do |f| %>
	<%= render 'shared/error_messages', :object => @new_portfolio %>
	<%= f.file_field :photo %><br/>
	<%= f.text_field :url%>
	<%= f.select :year, (1990..2050).to_a.map, :selected => Date.today.year %><br />
	<%= f.submit "Submit" %>
<% end %>
